    "core-js": "^3.8.2",
    "tiptap": "^1.32.1",
    "tiptap-extensions": "^1.35.1",
    "vue": "^2.6.12",
    "vue-i18n": "^8.22.3",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.4.9",
    "vuetify": "^2.4.2",
    "vuetify-dialog": "^2.0.13",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-eslint": "^4.5.10",
    "@vue/cli-plugin-router": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
