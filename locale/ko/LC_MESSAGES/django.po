#: frontend/templates/password_reset.html:19
#, python-format
msgid "Dear %(call_name)s"
msgstr "%(call_name)s 님"

#: frontend/templates/password_reset.html:25
msgid "Someone requested that the password for your account be reset."
msgstr "비밀번호 초기화 요청에 의해 발송된 메일입니다."

#: frontend/templates/password_reset.html:31
msgid "Please go to the following page to set a new password."
msgstr "아래 링크에 접속하여 새로운 비밀번호를 설정하세요."

#: frontend/templates/password_reset.html:40
#: frontend/templates/password_reset_subject.txt:2
msgid "Password reset"
msgstr "비밀번호 초기화"

#: frontend/templates/password_reset.html:49
msgid ""
"If you didn't request this, you can ignore this email. Your password won't "
"be changed until you set a new password."
msgstr ""
"만약 비밀번호 초기화 요청을 한적이 없다면, 이 메일은 무시하셔도 됩니다. 비밀"
"번호를 새로 설정하지 않는한 기존 비밀번호는 변경되지 않습니다."

#: utils/text.py:11
msgid "Unable to login."
msgstr "로그인 실패"

#: utils/text.py:12
msgid "User is deactivated."
msgstr "탈퇴한 사용자입니다."

#: utils/text.py:13
msgid "Invalid password."
msgstr "비밀번호가 잘못되었습니다."

#: utils/text.py:14
msgid "Same as old password."
msgstr "이전 비밀번호와 동일합니다."

#: utils/text.py:15
msgid "User not exist."
msgstr "존재하지 않는 사용자입니다."

#: utils/text.py:16
msgid "Invalid uid."
msgstr "사용자 아이디 오류"

#: utils/text.py:17
msgid "Invalid token."
msgstr "토큰 오류"

#: utils/text.py:18
msgid "You must consent."
msgstr "동의해야 합니다."

#: utils/text.py:20
msgid "Invalid permission type"
msgstr "권한이 잘못되었습니다."

#: utils/text.py:21
msgid "alphabets and number only"
msgstr "알파벳 대소문자와 숫자만 가능합니다."

#: utils/text.py:23
msgid "This field is required."
msgstr "이 필드는 필수 항목입니다."

#: utils/text.py:24
msgid "Invalid value"
msgstr "유효하지 않은 값입니다."

#: utils/text.py:25
msgid "File too large"
msgstr "파일이 너무 큽니다."

#: workcloud/settings.py:241
msgid "Korean"
msgstr "한국어"

#: workcloud/settings.py:242
msgid "English"
msgstr "영어"
